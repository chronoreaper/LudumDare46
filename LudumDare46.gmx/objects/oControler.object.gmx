{
    toBuildIndex ++
    if toBuildIndex &gt;= ds_list_size(toBuildList)
        toBuildIndex = 0
    toBuild = toBuildList[|toBuildIndex]
}

if mouse_wheel_down(){
    toBuildIndex --
    if toBuildIndex &lt; 0 
        toBuildIndex =  ds_list_size(toBuildList) - 1
    toBuild = toBuildList[|toBuildIndex]
}

//skip wait
if alarm_get(0)&gt;1
